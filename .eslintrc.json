{
  "root": true,
  "extends": [
    "airbnb-base",
    "prettier",
    "plugin:node/recommended"
  ],
  "plugins": [
    "prettier"
  ],
  "rules": {
    "prettier/prettier": "error",
    "no-unused-vars": "warn",
    "no-console": "off",
    "func-names": "warn",
    "no-process-exit": "off",
    "object-shorthand": "warn",
    "class-methods-use-this": "warn"
  }
}